{
    "manifest_version": 2,

    "name": "mamadata",
    "description": "mamadata",
    "version": "3.1.3",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon_off.png"
    },
    "content_scripts": [{
        "matches": ["https://*.taobao.com/*", "http://*.taobao.com/*", "http://*.alimama.com/*", "https://*.alimama.com/*", "https://*.lazada.com.ph/*", "https://*.alibaba-inc.com/*"],
        "js": ["jquery-2.0.0.min.js", "underscore.min.js", "clipboard.min.js", "content.js"],
        "css": ["content.css"]
    }],
    "permissions": [
        "https://fether.m.alibaba-inc.com/"
    ]
}