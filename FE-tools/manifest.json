{
    "manifest_version": 2,

    "name": "mamadata",
    "description": "mamadata",
    "version": "3.1.0",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon_off.png"
    },
    "content_scripts": [{
        "matches": ["https://*.taobao.com/*", "http://*.taobao.com/*", "http://*.alimama.com/*", "https://*.alimama.com/*","https://sellercenter-staging.lazada.com.ph/*", "https://sellercenter.lazada.com.ph/*"],
        "js": ["jquery-2.0.0.min.js", "underscore.min.js", "clipboard.min.js", "content.js"],
        "css": ["content.css"]
    }],
    "permissions": [
        "activeTab",
        "https://*.taobao.com/*",
        "https://*.alimama.net/*",
        "https://sellercenter-staging.lazada.com.ph/*",
        "https://sellercenter.lazada.com.ph/*"
    ]
}